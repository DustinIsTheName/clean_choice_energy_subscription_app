<% if current_user.access == "customer_care" or current_user.access == "admin" %>
  
  <% if false %>
    <% page_limit = 5 %>

    <div class="page-content subscription-page" data-empty-subscription-row="<%= empty_subscription_row.gsub("\n", " ") %>">
      <div class="container page-title-container">
        <div class="page-title align-left">Subscriptions</div>
        <button id="reveal-add-single-subscription" class="align-right">Add Subscription</button>
      </div><!-- .container -->
      <div class="container">
        <div class="add-container clearfix" style="display: none;">
          <div class="add-title">Add Subscription</div>
          <div class="add-subscription-form clearfix">
            <div class="add-subscription-fields align-left">
              <input class="single-subscription-field" type="text" name="First Name" placeholder="First Name" tabindex="1"/>
              <input class="single-subscription-field" type="text" name="Street Address" placeholder="Street Address" tabindex="4"/>
              <input class="single-subscription-field" type="text" name="Credit Card #" placeholder="Credit Card #" tabindex="9"/>
              <input class="single-subscription-field" type="text" name="Last Name" placeholder="Last Name" tabindex="2"/>
              <input class="single-subscription-field" type="text" name="Street Address 2" placeholder="Street Address 2" tabindex="5"/>
              <input class="single-subscription-field" type="text" name="Credit Card Expiration (MM/YY)" placeholder="Expiration (MM/YY)" tabindex="10"/>
              <input class="single-subscription-field" type="email" name="Email" placeholder="Email Address" tabindex="3"/>
              <input class="single-subscription-field" type="text" name="City" placeholder="City" tabindex="6"/>
              <div class="dropdown">
                <select class="single-subscription-field" tabindex="11" name="Subscription Product">
                  <option selected="true" disabled="disabled">Subscription Product</option>
                  <option value="1602141356074">NREC</option>
                  <option value="1602140700714">Clean Furnace</option>
                </select>
              </div><!-- .dropdown -->
              <div class="dropdown">
                <select class="single-subscription-field" tabindex="7" name="State">
                  <option selected="true" disabled="disabled">State</option>
                  <option value="AL">Alabama</option>
                  <option value="AK">Alaska</option>
                  <option value="AZ">Arizona</option>
                  <option value="AR">Arkansas</option>
                  <option value="CA">California</option>
                  <option value="CO">Colorado</option>
                  <option value="CT">Connecticut</option>
                  <option value="DE">Delaware</option>
                  <option value="DC">District Of Columbia</option>
                  <option value="FL">Florida</option>
                  <option value="GA">Georgia</option>
                  <option value="HI">Hawaii</option>
                  <option value="ID">Idaho</option>
                  <option value="IL">Illinois</option>
                  <option value="IN">Indiana</option>
                  <option value="IA">Iowa</option>
                  <option value="KS">Kansas</option>
                  <option value="KY">Kentucky</option>
                  <option value="LA">Louisiana</option>
                  <option value="ME">Maine</option>
                  <option value="MD">Maryland</option>
                  <option value="MA">Massachusetts</option>
                  <option value="MI">Michigan</option>
                  <option value="MN">Minnesota</option>
                  <option value="MS">Mississippi</option>
                  <option value="MO">Missouri</option>
                  <option value="MT">Montana</option>
                  <option value="NE">Nebraska</option>
                  <option value="NV">Nevada</option>
                  <option value="NH">New Hampshire</option>
                  <option value="NJ">New Jersey</option>
                  <option value="NM">New Mexico</option>
                  <option value="NY">New York</option>
                  <option value="NC">North Carolina</option>
                  <option value="ND">North Dakota</option>
                  <option value="OH">Ohio</option>
                  <option value="OK">Oklahoma</option>
                  <option value="OR">Oregon</option>
                  <option value="PA">Pennsylvania</option>
                  <option value="RI">Rhode Island</option>
                  <option value="SC">South Carolina</option>
                  <option value="SD">South Dakota</option>
                  <option value="TN">Tennessee</option>
                  <option value="TX">Texas</option>
                  <option value="UT">Utah</option>
                  <option value="VT">Vermont</option>
                  <option value="VA">Virginia</option>
                  <option value="WA">Washington</option>
                  <option value="WV">West Virginia</option>
                  <option value="WI">Wisconsin</option>
                  <option value="WY">Wyoming</option>
                </select>
              </div><!-- .dropdown -->
              <input class="single-subscription-field" type="text" name="Zip" placeholder="Zip" tabindex="8"/>
            </div><!-- .add-subscription-fields -->
            <button id="add-single-subscription" class="align-right" tabindex="12">Add</button>
          </div><!-- .add-subscription-form -->
        </div><!-- .add-container -->
      </div><!-- .container -->
      <div class="subscriptions-filter-bar">
        <div class="container clearfix">
          <div class="subscriptions-filter align-left">
            <div class="subscriptions-filter-label align-left">Filter</div>
            <div class="subscriptions-filter-form align-left">
              <ul class="email-filter">
                <li class="active" data-filter=""><button>All</button></li>
                <li data-filter="hide-email"><button>Email</button></li>
                <li data-filter="hide-no-email"><button>No Email</button></li>
              </ul>
            </div><!-- .subscriptions-filter-form -->
          </div><!-- .subscriptions-filter -->
          <div class="subscriptions-filter align-right">
            <div class="subscriptions-filter-label align-left">Search by Customer Name</div>
            <div class="subscriptions-filter-form align-left">
              <input class="search-input" type="text" placeholder="ex: John Doe" />
              <button class="align-right search">Search</button>
            </div><!-- .subscriptions-filter-form -->
          </div><!-- .subscriptions-filter -->
        </div><!-- .container -->
      </div><!-- .subscriptions-filter-bar -->
      <div class="container">
        <div class="subscriptions clearfix">
          <div class="subs-header clearfix">
            <ul>
              <li>#</li>
              <li>Name</li>
              <li>Email</li>
              <li>Product</li>
              <li>Amount</li>
              <li>CC #</li>
              <li>Enroll Date</li>
              <li></li>
            </ul>
          </div><!-- .subs-header -->
          <% for subscription in @subscriptions %>
            <%= render partial: '/partials/subscription_row', locals: {subscription: subscription} %>
          <% end %>
          
        </div><!-- subscriptions -->
        <% if @load_more %>
          <button class="load-more">Load more</button>
        <% end %>
      </div><!-- .container -->
    </div><!-- .page-content -->
  <% else %>

    <div class="page-content">
      <div class="container">
        <div class="page-title">Subscription Coming Soon</div>
      </div> 
    </div>

  <% end %>
<% else %>
  <%= render partial: '/partials/permission' %>
<% end %>